{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "docsy/contact-form",
	"version": "0.1.0",
	"title": "Contact Form",
	"category": "theme",
	"icon": "email-alt",
	"description": "A customizable contact form block with title, subtitle, and Contact Form 7 integration.",
	"example": {},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Send us a Message"
		},
		"subtitle": {
			"type": "string",
			"default": "Fill out the form below and we'll get back to you as soon as possible"
		},
		"formShortcode": {
			"type": "string",
			"default": "[contact-form-7 id=\"0b37750\" title=\"Contact form 1\"]"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "docsy",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}